{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2021",
        "noImplicitAny": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": false
    },
    "include": ["src/**/*"]
}
